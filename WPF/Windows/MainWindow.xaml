<Window x:Class="WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local2="clr-namespace:WPF.Components"
        xmlns:local3="clr-namespace:WPF.Converters"
        mc:Ignorable="d"
        Title="List of all students" WindowStartupLocation="CenterScreen" Height="450" Width="800">

    <Window.Resources>
        <local3:PasswordMaskConverter x:Key="PasswordMaskConverter"/>
    </Window.Resources>
    
    
    <Grid>
        <local2:StudentList x:Name="studentList" Padding="2" />
    </Grid>
</Window>
